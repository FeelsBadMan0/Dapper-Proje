@using EntityLayer.Concrete
@model List<Education>
<div class="resume-section-content">
    <h2 class="mb-5">Eğitimler</h2>
    @foreach (var edc in Model)
    {
        <div class="d-flex flex-column flex-md-row justify-content-between mb-5">
            <div class="flex-grow-1">
                <h3 class="mb-0">@edc.Title</h3>
                <div class="subheading mb-3">@edc.SubTitle</div>
                <div>@edc.Description</div>
                <p>GNOT: @edc.GNOT</p>
            </div>
            <div class="flex-shrink-0"><span class="text-primary">@edc.Date</span></div>
        </div>
    }

    
</div>